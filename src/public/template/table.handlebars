{{#if producto}}
<table>
    <thead>
        <th>Producto</th>
        <th>Precio</th>
        <th>Imagen</th>
    </thead>
    <body>
        {{#each producto}}
        <tr>
            <td>{{this.titulo}}</td>
            <td>{{this.price}}</td>
            <td><img src={{this.thumbnail}} style="width: 100px;"></td>
        </tr>
        {{/each}}
    </body>
</table>
{{else}}
<p>No hay Productos</p>
{{/if}}
 